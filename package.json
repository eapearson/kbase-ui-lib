        "enzyme-adapter-react-16": "1.14.0",
        "eslint": "6.0.1",
        "eslint-plugin-react": "7.14.2",
        "jest": "24.8.0",
        "typescript": "3.5.2",
        "ts-jest": "24.0.2",
        "jest-transform-css": "2.0.0",
