    "homepage": "https://github.com/kbaseIncubator/kbase-ui-lib#readme",
    "dependencies": {
        "@types/bluebird": "^3.5.27",
        "@types/react": "^16.8.19",
        "@types/react-dom": "^16.8.4",
        "@types/react-redux": "^7.0.9",
        "@types/uuid": "^3.4.4",
        "antd": "^3.19.3",
        "bluebird": "^3.5.5",
        "es-cookie": "^1.2.0",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
